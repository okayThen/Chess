
public class Game
{
    public Game()
    {
        Board board = new Board();
        
        while(true)
        {
            whiteTurn();
            if(findWinner().equals("white");
                System.out.println("Game over" + "\n" + "White wins");
            
            blackTurn();
            if(findWinner().equals("black");
                System.out.println("Game over" + "\n" + "Black wins");
        }
    }
    
    

}
